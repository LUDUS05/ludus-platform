# LUDUS Frontend Production Environment
# Copy this file to .env.production and update with your production values

# Backend API URL (update with your Railway deployment URL)
REACT_APP_API_URL=https://your-backend-url.railway.app

# Moyasar Production Keys
REACT_APP_MOYASAR_PUBLISHABLE_KEY=pk_live_your_production_publishable_key

# Google Maps API Key
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Sentry Configuration (Optional)
REACT_APP_SENTRY_DSN=your_sentry_dsn

# Environment
REACT_APP_ENVIRONMENT=production